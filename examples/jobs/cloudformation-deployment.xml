<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.jira.JiraProjectProperty plugin="jira@3.0.13"/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>environment_id</name>
          <description>ID of environment to create (optional; automatically generated if not provided)</description>
          <defaultValue></defaultValue>
          <trim>true</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>region_name</name>
          <description>AWS region to use</description>
          <defaultValue></defaultValue>
          <trim>true</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>access_key_id</name>
          <description>Access Key ID to use</description>
          <defaultValue></defaultValue>
          <trim>true</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.PasswordParameterDefinition>
          <name>secret_access_key</name>
          <description>Secret access key to use</description>
          <defaultValue>{AQAAABAAAAAQKBLcgVIX0a4+4vBxABtK0csWG8miCIr4VsP7YMDzlUo=}</defaultValue>
        </hudson.model.PasswordParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>stack_name</name>
          <description>Name of the new stack</description>
          <defaultValue></defaultValue>
          <trim>true</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>template_url</name>
          <description>URL of Terraform template</description>
          <defaultValue></defaultValue>
          <trim>true</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>parameters</name>
          <description>Template parameters (in YAML/JSON format)</description>
          <defaultValue></defaultValue>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.FileParameterDefinition>
          <name>cfn_vars</name>
          <description>YAML/JSON file containing template parameters</description>
        </hudson.model.FileParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <co.cloudify.jenkins.plugin.integrations.CloudFormationBuildStep plugin="cloudify-jenkins-plugin@1.0-SNAPSHOT">
      <deploymentId>${environment_id}</deploymentId>
      <echoEnvData>true</echoEnvData>
      <debugOutput>false</debugOutput>
      <accessKeyId>${access_key_id}</accessKeyId>
      <secretAccessKey>${secret_access_key}</secretAccessKey>
      <regionName>${region_name}</regionName>
      <stackName>${stack_name}</stackName>
      <parameters>${parameters}</parameters>
      <parametersFile>${cfn_vars}</parametersFile>
      <templateUrl>${template_url}</templateUrl>
    </co.cloudify.jenkins.plugin.integrations.CloudFormationBuildStep>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>