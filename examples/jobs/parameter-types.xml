<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Very simple job to demonstrate how the Cloudify-provided parameter types work:&#xd;
&#xd;
* Blueprint Selector&#xd;
* Deployment Selector&#xd;
* Constrained Input Value Selector</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <co.cloudify.jenkins.plugin.parameters.BlueprintSelectorParameterDefinition plugin="cloudify-jenkins-plugin@1.0-SNAPSHOT">
          <name>blueprint_id</name>
          <description></description>
          <filter></filter>
        </co.cloudify.jenkins.plugin.parameters.BlueprintSelectorParameterDefinition>
        <co.cloudify.jenkins.plugin.parameters.DeploymentSelectorParameterDefinition plugin="cloudify-jenkins-plugin@1.0-SNAPSHOT">
          <name>deployment_id</name>
          <description></description>
          <blueprintId></blueprintId>
        </co.cloudify.jenkins.plugin.parameters.DeploymentSelectorParameterDefinition>
        <co.cloudify.jenkins.plugin.parameters.BlueprintInputConstraintSelector plugin="cloudify-jenkins-plugin@1.0-SNAPSHOT">
          <name>selected_value</name>
          <description></description>
          <blueprintId>app-nodejs</blueprintId>
          <inputName>infra_name</inputName>
        </co.cloudify.jenkins.plugin.parameters.BlueprintInputConstraintSelector>
        <co.cloudify.jenkins.plugin.parameters.WorkflowSelectorParameterDefinition plugin="cloudify-jenkins-plugin@1.0-SNAPSHOT">
          <name>workflow_id</name>
          <description></description>
          <deploymentId>dep1</deploymentId>
        </co.cloudify.jenkins.plugin.parameters.WorkflowSelectorParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>echo &quot;Selected blueprint ID: ${blueprint_id}&quot;
echo &quot;Selected deployment ID: ${deployment_id}&quot;
echo &quot;Selected constrained value: ${selected_value}&quot;
echo &quot;Selected workflow ID: ${workflow_id}&quot;
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>